<h1> Saved NFL Articles </h1>
{{#each headlines}}
{{#if saved}}
<div id="articles">
    <h5> {{title}} </h5>
</div>

<p> {{summary}}
<a href ={{link}}> Read More! </a>
</p>


   <div id="notes"></div>


  <button class="btn btn-success delete" data-id="{{id}}"> Delete This Article </button>

<button class="btn btn-success notes"
data-id="{{id}}"> Article Notes </button>
              
              
{{#each note}}
<p> Hello {{body}} </p>
{{/each}}

 {{/if}}
{{/each}}

